{
    "name": "dvicklund/inventory",
    "description": "Inventory management using Laravel 8",
    "keywords": ["laravel", "inventory", "inventory-management"],
    "authors": [
        {
            "name": "Steve Bauman",
            "email": "steven_bauman@outlook.com"
        },
        {
            "name": "David Vicklund",
            "email": "david@vicklund.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.3.0",
        "laravel/framework": "^8.0",
        "gazsp/baum": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.0"
    },
    "archive": {
      "exclude": ["/tests"]
    },
    "autoload": {
        "psr-4": {
            "Stevebauman\\Inventory\\": "src/"
        }
    },
    "autoload-dev":
    {
        "psr-4": {
          "Stevebauman\\Inventory\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
