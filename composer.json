{
    "name": "dvicklund/inventory",
    "description": "Inventory management using Laravel 9|10|11",
    "keywords": ["laravel", "inventory", "inventory-management"],
    "authors": [
        {
            "name": "Steve Bauman",
            "email": "steven_bauman@outlook.com"
        },
        {
            "name": "David Vicklund",
            "email": "david@vicklund.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0.2 || ^8.1 || ^8.2",
        "laravel/framework": "^9.6 || ^10.0 || ^11.0",
        "gazsp/baum": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.8 || ^10.0.7",
        "mockery/mockery": "^1.0",
        "fakerphp/faker": "^1.21",
        "orchestra/testbench": "^7.24 || ^8.23.2 || ^9.0",
        "brianium/paratest": "^6.0",
        "nunomaduro/collision": "^6.0 || ^7.0 || ^8.0"
    },
    "archive": {
        "exclude": ["/tests"]
    },
    "autoload": {
        "psr-4": {
            "Stevebauman\\Inventory\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stevebauman\\Inventory\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Stevebauman\\Inventory\\InventoryServiceProvider"
            ]
        }
    }
}
